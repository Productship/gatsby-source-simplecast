{
  "name": "gatsby-source-simplecast",
  "version": "0.0.0",
  "engines": {
    "node": ">=10.5.0"
  },
  "description": "A source plugin to add data from Simplecast API into GraphQL",
  "main": "./build/gatsby-node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ebinion/gatsby-source-simplecast.git"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint source/** --fix",
    "test": "npm run lint",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "eslint-staged",
      "git add"
    ]
  },
  "author": "Ezekiel Binion <ebinion@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "gatsby-node-helpers": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-staged": "^1.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "1.13.7",
    "prettier-eslint": "^8.8.2",
    "pretty-quick": "^1.6.0"
  }
}
