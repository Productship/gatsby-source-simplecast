{
  "name": "gatsby-source-simplecast",
  "version": "0.0.1",
  "engines": {
    "node": ">=10.5.0"
  },
  "description": "A source plugin to add data from Simplecast API into GraphQL",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "simplecast"
  ],
  "main": "gatsby-node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ebinion/gatsby-source-simplecast.git"
  },
  "scripts": {
    "build": "npx babel source -d .",
    "develop": "npx babel source -d . --watch",
    "lint": "npx eslint source/** --fix",
    "test": "npm run lint",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "eslint-staged",
      "git add"
    ]
  },
  "author": "Ezekiel Binion <ebinion@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "gatsby-node-helpers": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.4.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-staged": "^1.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "1.13.7",
    "prettier-eslint": "^8.8.2",
    "pretty-quick": "^1.6.0"
  }
}
